    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "fast-glob": "^3.3.3",
    "node-cron": "^3.0.3",
    "play-dl": "^1.9.7",
    "spotify-web-api-node": "^5.0.2",
    "winston": "^3.13.0",
    "yt-search": "^2.13.1",
    "zod": "^3.23.8",
    "@cyberhub/shared": "file:../../packages/shared"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "tsup": "^8.1.0",
    "tsx": "^4.15.4",
